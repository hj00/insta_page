<h1>Edit Post</h1>
<%= form_tag(post_path(@post.id), method: "put") do %>
    <%= label_tag(:image_url, "Image : ")%>
    <%= text_field_tag(:image_url, @post.image_url) %><br/>
    <%= label_tag(:story, "Story") %><br/>
    <%= text_area_tag(:story, @post.story) %>
    <%= submit_tag("EDIT")%>
<% end %>